<h1>Listing custos</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Meta</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @custos.each do |custo| %>
  <tr>
    <td><%= custo.nome %></td>
    <td><%= custo.meta %></td>
    <td><%= link_to 'Show', custo %></td>
    <td><%= link_to 'Edit', edit_custo_path(custo) %></td>
    <td><%= link_to 'Destroy', custo, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Custo', new_custo_path %>
